            <body><![CDATA[CONST_UNDEFINED = 0
CONST_YES = 1
CONST_NO = 2
CONST_NOT_APPLICABLE  = 3

#
# Calculates the MRL level of a sub thread
#
def calculateMrlLevels(subThreadNum):
  for index in range(0, numSubThreads):
    currentLevel = targetLevel.getValue()
    subThreadLevels.setItem(index, getSubThreadLevel(index, currentLevel))

def getSubThreadLevel(subThreadNum, level):
  boolFoundUndefined = False
  boolFoundYes = False
  boolFoundNo = False
  boolFoundNotApplicable = False
    
  #  
  # Loop through all the questions
  # Only pay attention to the ones at the current level
  #
  for index in range(0, numQuestions):
    answerLevel = questionLevels.getItem(index)
    answerSubThreadNum = questionSubThreads.getItem(index)
    
    if answerLevel == level and answerSubThreadNum == subThreadNum:
      answerVal = questionAnswers.getItem(index)
      print "Answer:", answerVal
      if answerVal == CONST_UNDEFINED:
        boolFoundUndefined = True
      if answerVal == CONST_YES:
        boolFoundYes = True
      if answerVal == CONST_NO:
        boolFoundNo = True
      if answerVal == CONST_NOT_APPLICABLE:
        boolFoundNotApplicable = True
  
  if(boolFoundUndefined):
    print "Undefined"
    return CONST_UNDEFINED
  if(boolFoundNo):
    print "No"
    return CONST_NO
  if(boolFoundYes):
    print "Yes"
    return CONST_YES
  print "Didn't find anything"
  return CONST_UNDEFINED

#
# Main code starts here
#
calculateMrlLevels(0)]]></body>
